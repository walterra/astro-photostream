{
  "name": "astro-photo-stream",
  "version": "0.1.0",
  "type": "module",
  "description": "An Astro integration for creating sophisticated photo galleries and streams with AI-powered metadata generation and geolocation features",
  "keywords": [
    "astro-integration",
    "withastro",
    "astro-component",
    "astro",
    "photo",
    "gallery",
    "stream",
    "photography",
    "photos",
    "image",
    "images", 
    "metadata",
    "exif",
    "ai",
    "geolocation",
    "map",
    "responsive",
    "seo",
    "content-collections",
    "typescript"
  ],
  "homepage": "https://github.com/walterra/astro-photostream",
  "repository": {
    "type": "git",
    "url": "https://github.com/walterra/astro-photostream.git"
  },
  "bugs": {
    "url": "https://github.com/walterra/astro-photostream/issues"
  },
  "author": "Walter Rafelsberger",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "astro-photo-stream": "./dist/scripts/photo-metadata-generator.js",
    "photo-metadata-generator": "./dist/scripts/photo-metadata-generator.js"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc --watch",
    "check": "astro check",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write src",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "peerDependencies": {
    "astro": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "astro-integration-kit": "^0.19.0",
    "zod": "^3.22.4",
    "exifr": "^7.1.3",
    "sharp": "^0.33.2",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "astro": "^5.13.2",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}