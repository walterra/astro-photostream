{
  "name": "astro-photostream",
  "version": "0.2.2",
  "type": "module",
  "description": "An Astro integration for creating photo galleries with AI metadata generation and geolocation features",
  "keywords": [
    "astro-integration",
    "withastro",
    "astro-component",
    "astro",
    "photo",
    "gallery",
    "stream",
    "photography",
    "photos",
    "image",
    "images",
    "metadata",
    "exif",
    "ai",
    "geolocation",
    "map",
    "responsive",
    "seo",
    "content-collections",
    "typescript"
  ],
  "homepage": "https://github.com/walterra/astro-photostream",
  "repository": {
    "type": "git",
    "url": "https://github.com/walterra/astro-photostream.git"
  },
  "bugs": {
    "url": "https://github.com/walterra/astro-photostream/issues"
  },
  "author": "Walter Rafelsberger",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./components": {
      "types": "./src/components/index.ts",
      "import": "./src/components/index.ts"
    },
    "./schema": {
      "types": "./src/schema.ts",
      "import": "./src/schema.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "import": "./src/utils/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "astro-photostream": "./dist/scripts/photo-metadata-generator.js",
    "photo-metadata-generator": "./dist/scripts/photo-metadata-generator.js"
  },
  "files": [
    "src",
    "dist/scripts",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build:cli": "tsc src/scripts/photo-metadata-generator.ts --outDir dist/scripts --moduleResolution bundler --target ES2022 --module ESNext --allowSyntheticDefaultImports --esModuleInterop --skipLibCheck",
    "dev": "astro check --watch",
    "check": "astro check",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write src",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "version": "changeset version",
    "release": "pnpm build:cli && changeset publish",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:status": "changeset status"
  },
  "peerDependencies": {
    "astro": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.25.0",
    "astro-integration-kit": "^0.19.0",
    "exifr": "^7.1.3",
    "gray-matter": "^4.0.3",
    "sharp": "^0.33.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@changesets/cli": "^2.27.1",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "astro": "^5.13.2",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}