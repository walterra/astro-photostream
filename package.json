{
  "name": "astro-photostream",
  "version": "0.3.7",
  "type": "module",
  "description": "An Astro integration for creating photo galleries with AI metadata generation and geolocation features",
  "keywords": [
    "astro-integration",
    "withastro",
    "astro-component",
    "astro",
    "photo",
    "gallery",
    "stream",
    "photography",
    "photos",
    "image",
    "images",
    "metadata",
    "exif",
    "ai",
    "geolocation",
    "map",
    "responsive",
    "seo",
    "content-collections",
    "typescript"
  ],
  "homepage": "https://github.com/walterra/astro-photostream",
  "repository": {
    "type": "git",
    "url": "https://github.com/walterra/astro-photostream.git"
  },
  "bugs": {
    "url": "https://github.com/walterra/astro-photostream/issues"
  },
  "author": "Walter Rafelsberger",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./components": {
      "types": "./src/components/index.ts",
      "import": "./src/components/index.ts"
    },
    "./schema": {
      "types": "./src/schema.ts",
      "import": "./src/schema.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "import": "./src/utils/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "astro-photostream": "./dist/scripts/photo-metadata-generator.js",
    "photo-metadata-generator": "./dist/scripts/photo-metadata-generator.js"
  },
  "files": [
    "src",
    "dist/scripts",
    "scripts",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build:cli": "tsc src/scripts/photo-metadata-generator.ts --outDir dist --moduleResolution bundler --target ES2022 --module ESNext --allowSyntheticDefaultImports --esModuleInterop --skipLibCheck",
    "dev": "astro check --watch",
    "check": "astro check",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "quality": "pnpm lint && pnpm format:check && pnpm check",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "commit": "git-cz",
    "version": "changeset version",
    "release": "pnpm build:cli && changeset publish",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:status": "changeset status",
    "postinstall": "node scripts/download-fonts.js",
    "prepare": "husky"
  },
  "peerDependencies": {
    "astro": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@resvg/resvg-js": "^2.6.2",
    "astro-integration-kit": "^0.19.0",
    "dotenv": "^17.2.2",
    "exifr": "^7.1.3",
    "gray-matter": "^4.0.3",
    "satori": "^0.19.1",
    "sharp": "^0.34.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@eslint/js": "^9.34.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "astro": "^5.13.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.34.0",
    "globals": "^17.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.2.4",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.41.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
